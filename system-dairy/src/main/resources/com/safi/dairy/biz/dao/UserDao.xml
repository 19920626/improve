<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.safi.dairy.biz.dao.UserDao">
    <select id="getAllUsers" resultType="com.safi.dairy.biz.domain.User">
        SELECT
        USER_ID AS userId,
        USER_NAME AS userName
        FROM USER;
    </select>
</mapper>